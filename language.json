    "usage": {
        "en": {
            "leave" : {
                "Alias"         : ["leave", "exit", "quit"],
                "Description"   : ["Leave the configuration menu."],
                "Arguments"     : ["None"]
            },

            "help" : {
                "Alias"         : ["help", "h", "-help", "-h", "--help", "--h", "?"],
                "Description"   : ["Display the help for all commands, or specific ones if used as argument."],
                "Arguments"     : ["all -> Display the help for windows native commands."]
            },

            "setup" : {
                "Alias"         : ["setup", "config", "configure"],
                "Description"   : ["Display the configuration menu."],
                "Arguments"     : ["None"]
            },

            "randnum" : {
                "Alias"         : ["randnum", "randint", "randomint", "random", "rdint", "rd", "rand"],
                "Description"   : ["Generate a random integer between two values."],
                "Arguments"     : ["The minimum value., or the maximum if only one argument.", "The maximum value.", "type -> By default integer, but you can specify float."]
            },

            "art" : {
                "Alias"         : ["art", "1-line"],
                "Description"   : ["Display a 1-line art. Credit to https://github.com/sepandhaghighi/art"],
                "Arguments"     : ["name -> The name of the art, if the name is random, get a random art."]
            },

            "calc" : {
                "Alias"         : ["calc", "calculator", "calcul", "calculatrice"],
                "Description"   : ["Do your math."],
                "Arguments"     : ["The math you want to do, like 5 + 4."]
